{
  "name": "notegraf-web",
  "version": "0.1.0-alpha.2",
  "description": "The reference web frontend of Notegraf",
  "license": "AGPL-3.0-only",
  "private": true,
  "source": "src/frontend/index.html",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "scripts": {
    "start": "parcel --open",
    "build": "cargo build --release && parcel build --public-url /static --no-cache && rm -r .parcel-cache",
    "check": "tsc --noEmit && cargo check",
    "test": "cargo test",
    "lint": "cargo clippy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caizixian/notegraf.git"
  },
  "author": "Zixian Cai",
  "bugs": {
    "url": "https://github.com/caizixian/notegraf/issues"
  },
  "homepage": "https://github.com/caizixian/notegraf",
  "dependencies": {
    "dompurify": "^2.3.8",
    "highlight.js": "^11.5.1",
    "katex": "^0.16.0",
    "marked": "^4.0.17",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/dompurify": "^2.3.3",
    "@types/katex": "^0.14.0",
    "@types/marked": "^4.0.3",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "http-proxy-middleware": "^2.0.6",
    "parcel": "^2.6.1",
    "postcss": "^8.4.14",
    "process": "^0.11.10",
    "tailwindcss": "^3.1.2",
    "typescript": "^4.7.3"
  }
}
