{
  "name": "notegraf-web",
  "version": "0.1.1",
  "description": "The reference web frontend of Notegraf",
  "license": "AGPL-3.0-only",
  "private": true,
  "source": "src/frontend/index.html",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "scripts": {
    "start": "parcel --open",
    "build": "cargo build --release && parcel build --public-url /static --no-cache && rm -r .parcel-cache",
    "check": "tsc --noEmit && cargo check",
    "test": "cargo test",
    "lint": "cargo clippy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caizixian/notegraf.git"
  },
  "author": "Zixian Cai",
  "bugs": {
    "url": "https://github.com/caizixian/notegraf/issues"
  },
  "homepage": "https://notegraf.zcai.org",
  "dependencies": {
    "dompurify": "^3.0.6",
    "highlight.js": "^11.9.0",
    "katex": "^0.16.10",
    "lodash": "^4.17.21",
    "marked": "^9.1.6",
    "marked-gfm-heading-id": "^3.1.3",
    "marked-highlight": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.9",
    "@types/dompurify": "^3.0.3",
    "@types/katex": "^0.16.7",
    "@types/lodash": "^4.17.0",
    "@types/marked": "^6.0.0",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.14",
    "http-proxy-middleware": "^2.0.6",
    "parcel": "^2.10.2",
    "postcss": "^8.4.38",
    "process": "^0.11.10",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  }
}
